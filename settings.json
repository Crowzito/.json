{
  "workbench.colorTheme": "Min Dark",
  "workbench.iconTheme": "symbols",
  "editor.guides.bracketPairs": true,
  "terminal.integrated.env.windows": {},
  "editor.formatOnSave": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "vscode-pets.petSize": "medium",
  "vscode-pets.petColor": "warrior",
  "vscode-pets.petType": "cockatiel",
  "vscode-pets.theme": "forest",
  "vscode-pets.throwBallWithMouse": true,
  "explorer.compactFolders": false,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "workbench.startupEditor": "none",
  "notebook.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "git.enableSmartCommit": true,
  "editor.accessibilitySupport": "off",
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "'Fira Code', 'JetBrains Mono', 'JetBrainsMono Nerd Font'",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "graphql"
  ],
  "files.autoSave": "afterDelay",
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.lineHeight": 1.5,
  "editor.rulers": [80, 120],
  "editor.fontVariations": false,
  "symbols.hidesExplorerArrows": false,
  "editor.renderLineHighlight": "gutter",
  "workbench.editor.labelFormat": "short",
  "editor.semanticHighlighting.enabled": false,
  "breadcrumbs.enabled": false,
  "workbench.activityBar.location": "hidden",
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  // "window.titleBarStyle": "native",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    // "*.ts": "${capture}.js",
    // "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    // "*.jsx": "${capture}.js",
    // "*.tsx": "${capture}.ts",
    // "tsconfig.json": "tsconfig.*.json",
    // "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb"
    "package.json": ".eslint* , prettier*, tsconfig*, vite*, pnpm-lock*, package.lock*, bun.lock*",
  },
  "workbench.statusBar.visible": false,
  "apc.electron": {
    "titleBarStyle": "hiddenInset"
  },
  "apc.header": {
    "height": 36
  },
  "apc.font.family": "Inter",
  "apc.stylesheet": {
    ".title-label > h2": "display:none",
    ".editor-actions": "display: none",
    ".pane-body": "padding: 8px",
    ".pane-header": "padding: 0 8px",
  },
  "apc.listRow": {
    "height": 24
  },
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
}
